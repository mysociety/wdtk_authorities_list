                        },
                        "example": "//odsportal.digital.nhs.uk/Organisation/OrganisationDetails?organisationId=122014"
                    },
                    {
                        "name": "wdtk-url-name",
                        "type": "string",
                        "description": "The WDTK URL name of the authority",
                        "constraints": {
                            "unique": false
                        },
                        "example": "131_dental_rotherham"
                    }
                ]
            },
            "hash": "d6faa248615477c99eafcd408b175e5f"
        }
    ]
